<mxStylesheet>
	<add as="defaultVertex" extend="defaultVertex">
		<add as="fontSize" value="12" />
		<add as="fontFamily" value="Verdana" />
	</add>
	<add as="Host">
		<add as="type" value="Host" />
		<add as="fillColor" value="#fff" />
		<add as="strokeColor" value="#727272" />
		<add as="shape" value="swimlane" />
		<add as="startSize" value="50" />
		<add as="shadow" value="0" />
		<add as="resizable" value="0" />
	</add>

	<add as="Component">
		<add as="type" value="Component" />
		<add as="shape" value="image" />
		<add as="image" value="/resources/hw/theme/default/images/designbone_resource_background.png" />
		<add as="align" value="center" />
		<add as="connectable" value="1" />
		<add as="resizable" value="1" />
		<add as="rounded" value="1" />
	</add>

	<add as="Software">
		<add as="type" value="Software" />
		<add as="shape" value="image" />
		<add as="image" value="/resources/hw/theme/default/images/designbone_resource_background.png" />
		<add as="align" value="center" />
		<add as="connectable" value="0" />
		<add as="resizable" value="0" />
		<add as="rounded" value="1" />
	</add>

    <add as="Script">
        <add as="type" value="Script" />
        <add as="shape" value="image" />
        <add as="image" value="/resources/hw/theme/default/images/designbone_resource_background.png" />
        <add as="align" value="center" />
        <add as="connectable" value="0" />
        <add as="resizable" value="0" />
        <add as="rounded" value="1" />
    </add>

	<add as="VmTemplate">
		<add as="type" value="VmTemplate" />
		<add as="fillColor" value="#fff" />
		<add as="strokeColor" value="#727272" />
		<add as="shape" value="swimlane" />
		<add as="startSize" value="35" />
		<add as="shadow" value="0" />
		<add as="resizable" value="0" />
	</add>
	<add as="ScalingGroup">
		<add as="type" value="ScalingGroup"/>
		<add as="fillColor" value="#fff" />
		<add as="strokeColor" value="#727272" />
		<add as="shape" value="swimlane" />
		<add as="startSize" value="35" />
		<add as="shadow" value="0" />
		<add as="resizable" value="0" />
	</add>
	<add as="Network">
		<add as="type" value="Network" />
		<add as="shape" value="image" />
		<add as="align" value="right" />
		<add as="connectable" value="0" />
		<add as="resizable" value="0" />
		<add as="image" value="/resources/hw/theme/default/images/designerbone-network-icon.png"/>
		<add as="imageWidth" value="32"/>
		<add as="imageHeight" value="32"/>
		<add as="strokeColor" value="#727272" />
		<add as="fillColor" value="#727272"/>
		<add as="strokeWidth" value="1" />
	</add>
	<add as="defaultEdge" extend="defaultEdge">
		<add as="strokeColor" value="#adadad" />
		<add as="strokeWidth" value="2" />
		<add as="edgeStyle" value="elbowEdgeStyle" />
		<add as="rounded" value="true" />
	</add>
</mxStylesheet>
